{
  "name": "@uponu-solutions/jsonjoy-builder",
  "license": "MIT",
  "description": "Visual JSON Schema editor for creating and manipulating JSON Schema definitions with an intuitive interface.",
  "author": {
    "name": "UPONU Solutions"
  },
  "keywords": [
    "json",
    "schema",
    "editor",
    "visual",
    "react"
  ],
  "private": false,
  "version": "0.4.8",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/uponusolutions/jsonjoy-builder"
  },
  "files": [
    "dist",
    "!tsconfig.tsbuildinfo",
    "!tsconfig.js"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/jsonjoy-builder.d.ts",
  "exports": {
    ".": {
      "types": "./dist/jsonjoy-builder.d.ts",
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "module": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "rsbuild dev",
    "preview": "rsbuild preview",
    "build": "rslib build",
    "build:demo": "rsbuild build",
    "build:dev": "rslib build --mode development",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check .",
    "fix": "biome check --fix --unsafe",
    "typecheck": "tsc --build --force",
    "test": "tsx --tsconfig tsconfig.test.json --test test/**/*.test.{js,ts,tsx}",
    "test:snapshots": "tsx --test-update-snapshots --tsconfig tsconfig.test.json --test test/**/*.test.{js,ts,tsx}",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@monaco-editor/react": "^4.7.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.541.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@mantine/core": ">=8.3.9",
    "@mantine/hooks": ">=8.3.9",
    "monaco-editor": ">= 0.25.0 < 1",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@mantine/core": "^8.3.13",
    "@mantine/hooks": "^8.3.13",
    "@microsoft/api-extractor": "^7.55.5",
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-react": "^1.4.3",
    "@rslib/core": "^0.18.6",
    "@rspack/binding-linux-x64-gnu": "^1.7.4",
    "@tanstack/react-query": "^5.90.20",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "global-jsdom": "^27.0.0",
    "monaco-editor": "^0.55.1",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "sonner": "^2.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.24.0",
  "publishConfig": {
    "access": "public"
  }
}
